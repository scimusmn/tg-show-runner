{
  "name": "tg-temp2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gsap": "^1.20.4",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jquery": "^3.3.1",
    "node-localstorage": "^1.3.0",
    "normalize.css": "7.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.2",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5",
    "react-sparklines": "^1.7.0",
    "react-table": "^6.8.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "spectacle": "^4.0.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "archive-assets": "dateZip=assets-$(date +%Y_%m_%d_%H%M%S).zip; S3Bucket=s3://smm-depot/projects/tg/game-1-assets/tg-show-runner/archive/; zip -r $dateZip src/assets/images src/assets/sounds src/assets/videos && aws s3 cp $dateZip $S3Bucket && rm $dateZip && echo 'Archive created and uploaded to S3'",
    "push-assets": "aws s3 sync ./src/assets/ s3://smm-depot/projects/tg/game-1-assets/tg-show-runner/latest/ --exclude '*' --include 'images/*' --include 'sounds/*' --include 'videos/*' --delete",
    "pull-assets": "aws s3 sync s3://smm-depot/projects/tg/game-1-assets/tg-show-runner/latest/ ./src/assets/ --exclude '*' --include 'images/*' --include 'sounds/*' --include 'videos/*' --delete",
    "win:pull-assets": "aws s3 sync ./src/assets/ s3://smm-depot/projects/tg/game-1-assets/tg-show-runner/latest/ --exclude \"*\" --include \"images/*\" --include \"sounds/*\" --include \"videos/*\" --delete"
    "win:pull-assets": "aws s3 sync s3://smm-depot/projects/tg/game-1-assets/tg-show-runner/latest/ ./src/assets/ --exclude \"*\" --include \"images/*\" --include \"sounds/*\" --include \"videos/*\" --delete"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}


